{
  "name": "bazer-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "server": "babel-node ./src/bin/www",
    "server:prod": "node ./dist/bin/www",
    "dev": "SET NODE_ENV=development && npm-run-all watch",
    "clean": "rimraf dist",
    "build": "babel ./src --out-dir dist",
    "prod": "SET NODE_ENV=production && npm-run-all clean build server:prod",
    "watch": "nodemon"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "crypto": "^1.0.1",
    "debug": "~2.6.9",
    "express": "~4.17.3",
    "get-stream": "^5.1.0",
    "jimp": "^0.6.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.5.11",
    "morgan": "~1.9.1",
    "multer": "^1.4.1",
    "mysql2": "^1.6.5",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "sequelize": "^5.10.2",
    "sharp": "^0.22.1",
    "socket.io": "^2.3.0",
    "sqlite": "^3.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.4.5",
    "nodemon": "~1.19.0"
  }
}
